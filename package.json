    "@types/express": "^5.0.3",
    "@types/node": "^24.0.1",
    "@types/supertest": "^6.0.3",
    "@vitest/coverage-istanbul": "^3.2.3",
    "eslint": "^9.28.0",
    "globals": "^16.2.0",
    "husky": "^9.1.7",
    "msw": "^2.10.2",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.1",
    "ts-node": "^10.9.2",
    "typescript-eslint": "^8.34.0",
    "vitest": "^3.2.3"
